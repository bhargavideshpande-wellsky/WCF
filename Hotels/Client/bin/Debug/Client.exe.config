<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
    </startup>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="BasicHttpBinding_IHotel" />
            </basicHttpBinding>
            <wsHttpBinding>
                <binding name="WSHttpBinding_IHotel" />
            </wsHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost:8733/Design_Time_Addresses/Hotels/Hotel/basic"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IHotel"
                contract="HotelService.IHotel" name="BasicHttpBinding_IHotel" />
            <endpoint address="http://localhost:8733/Design_Time_Addresses/Hotels/Hotel/ws"
                binding="wsHttpBinding" bindingConfiguration="WSHttpBinding_IHotel"
                contract="HotelService.IHotel" name="WSHttpBinding_IHotel">
                <identity>
                    <userPrincipalName value="bdeshpande@internal.tavisca.com" />
                </identity>
            </endpoint>
        </client>
    </system.serviceModel>
